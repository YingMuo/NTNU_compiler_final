%%the beginning of an test data for Micro/Ex
Program testP
Begin
    declare I, O as integer;
    declare A,B,C,D, LLL[100] as float;
    FOR (I:=1 TO 100)
        A:=-LLL[I]+B*D-C;
        IF (A>=10000.0) THEN
            print(A+3.14);
        ELSE
            FOR (O:=1 TO 10)
                print(2,1.4);
            ENDFOR
        ENDIF
    ENDFOR
    
End